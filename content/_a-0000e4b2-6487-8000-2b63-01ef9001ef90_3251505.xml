    <versionUuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_277519</versionUuid>
    <rule>
        <name>EAP_Utils_CheckTextLengthValidation</name>
        <uuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251505</uuid>
        <description>Rule to check text length validation </description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      ri!Value
    ),
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      ri!ValidationLength
    )
  ),
  {},
  if(
    or(
      ri!IsCompareForMinimum
    ),
    if(
      len(
        ri!Value
      ) &lt; ri!ValidationLength,
      if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!ValidationMessage
        ),
        "Length should not be less than " &amp; ri!ValidationLength &amp; "." &amp; char(
          13
        ) &amp; " Current length is " &amp; len(
          ri!Value
        ),
        ri!ValidationMessage
      ),
      {}
    ),
    if(
      len(
        ri!Value
      ) &gt; ri!ValidationLength,
      if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!ValidationMessage
        ),
        "Length should not be more than " &amp; ri!ValidationLength &amp; "." &amp; char(
          13
        ) &amp; " Current length is " &amp; len(
          ri!Value
        ),
        ri!ValidationMessage
      ),
      {}
    )
  )
)</definition>
        <namedTypedValue>
            <name>Value</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>ValidationLength</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>ValidationMessage</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>IsCompareForMinimum</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
