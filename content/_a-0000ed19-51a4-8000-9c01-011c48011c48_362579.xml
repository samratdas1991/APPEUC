a!match(
  value : ri!spName,
  equals : "usp_EAP_TRNSFORM_SCHEDULED_DATA_PDF",
  then: "LP SCH A 1",
  equals : "usp_EAP_TRNSFORM_SCHEDULED_DATA_PDF_2",
  then: "LP SCH A 2",
  equals : "usp_EAP_TRNSFORM_SCHEDULED_DATA_PDF_3",
  then: "LP SCH A 3",
  equals : "usp_EAP_TRNSFORM_SCHEDULED_DATA_PDF_4",
  then: "LP SCH A 4",
  default: "empty"
)</definition>
        <namedTypedValue>
            <name>spName</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
