            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(isnull(ri!nullableValue), ri!replacementValue, ri!nullableValue)</definition>
        <namedTypedValue>
