<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4dd-0d8b-8000-5257-1bef901bef90_241727</versionUuid>
    <rule>
        <name>EAP_IOConfigDynamicValidations</name>
        <uuid>_a-0000e4dd-0d8b-8000-5257-1bef901bef90_241716</uuid>
        <description>Used to execute dynamic validations</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    ri!isReadOnly
  ),
  null,
  {
    if(
      or(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!minLength
        )
      ),
      {},
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251505"(
        value: ri!value,
        ValidationLength: ri!minLength,
        IsCompareForMinimum: true()
      )
    ),
    if(
      or(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!maxLength
        )
      ),
      {},
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251505"(
        value: ri!value,
        ValidationLength: ri!maxLength
      )
    ),
    #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_146004"(
      input: ri!validationExpression
    )
  }
)</definition>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>minLength</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>maxLength</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>validationExpression</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>isReadOnly</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
