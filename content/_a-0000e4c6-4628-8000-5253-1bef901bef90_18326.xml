        <definition>
  {a!localVariables(
    local!dwfprocessControllerData,{
  #"SYSTEM_SYSRULES_sectionLayout_v1"(
    label: upper("Transformation Template Configuration"),
    marginBelow: "NONE"
  ),
  #"_a-0000e4c6-e892-8000-5254-1bef901bef90_30016"(
    processId :ri!processId
  ),
  #"SYSTEM_SYSRULES_richTextDisplayField"(),
  if(
    ri!isUploadNewVersion=true(),
    #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_137302"(
      processId:ri!processId, 
      templateConfigId:ri!templateConfigid
    ),
    {}
  ),
  
  #"SYSTEM_SYSRULES_cardLayout"(
    contents: {
      /*Define Template Version*/
      #"SYSTEM_SYSRULES_columnsLayout"(
        columns: {
          #"SYSTEM_SYSRULES_columnLayout"(
            contents: {
              #"_a-0000e4c6-4628-8000-5253-1bef901bef90_16979"(
                processId: ri!processId,
                refProcessWFTemplateVersions: ri!refProcessWFTemplateVersions,
                isReadOnly: ri!isReadOnly,
                refProcessTempConfig : ri!refProcessTempConfig,
                runConfigId: ri!runConfigId,
                documentId : ri!documentId,
                dwfProcessControllerConfig:local!dwfprocessControllerData,
                templateConfigId: ri!templateConfigid
              )
            }
          )
        }
      ),
      /*Buttons*/
      #"SYSTEM_SYSRULES_ButtonLayout"(
        primaryButtons: {
          #"SYSTEM_SYSRULES_ButtonWidget"(
            label:if(
              ri!isUploadNewVersion=true(),
              "Submit",
              "Save &amp; Next"
            ) ,
            value: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_13888"[1],
            saveInto: {
              ri!buttonAction,
              if(
                or(
                  ri!activeStep &gt; 1
                ),
                {},
                {
                  a!save(
                    ri!activeStep,
                    ri!activeStep + 1
                  ),
                  a!save(
                    ri!selectedStep,
                    ri!steps[ri!activeStep]
                  )
                }
              )
            },
            submit: true(),
            size: "SMALL",
            style: "STANDARD",
            validate: true,
            showWhen: and(
              not(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251951"(ri!refProcessWFTemplateVersions)),
              length(ri!refProcessWFTemplateVersions)&gt;=1,
              if(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(local!dwfprocessControllerData),true,false())
            )
          )
       
          
        },
        secondaryButtons: {
          #"SYSTEM_SYSRULES_ButtonWidget"(
            label: "Cancel",
            value: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_13888"[2],
            saveInto: ri!buttonAction,
            size: "SMALL",
            style: "SECONDARY",
            submit: true(),
            validate:false()
          )
        }
      )
    },
    style: "INFO",
    showBorder: false()
  ),
  #"SYSTEM_SYSRULES_richTextDisplayField"(),
    #"SYSTEM_SYSRULES_sectionLayout_v1"(contents: {#"_a-0000e5c3-a65f-8000-5268-1bef901bef90_756378"(
      processControllerConfigData:local!dwfprocessControllerData,
      transformationId:1
    )},
    showWhen:if(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(local!dwfprocessControllerData),false(),true()))
})}</definition>
