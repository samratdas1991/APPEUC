<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e56b-0831-8000-5261-1bef901bef90_674275</versionUuid>
    <rule>
        <name>EAP_Utils_ConvertMinutesToTimeUnits</name>
        <uuid>_a-0000e56b-0831-8000-5261-1bef901bef90_674127</uuid>
        <description>Utility rule for converting input minutes into time unit format (Hr min)</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!hour: if(
    or(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
        ri!minutes
      ),
      ri!minutes &lt; 0
    ),
    {},
    floor(
      ri!minutes / 60
    )
  ),
  local!minutes: if(
    or(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
        ri!minutes
      ),
      ri!minutes &lt; 0
    ),
    {},
    mod(
      ri!minutes,
      60
    )
  ),
  local!timeUnit: concat(
    local!hour,
    " Hour(s) ",
    local!minutes,
    " Min(s)"
  ),
  if(
    or(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
        ri!minutes
      ),
      ri!minutes &lt; 0
    ),
    "Invalid Input",
    local!timeUnit
  )
)</definition>
        <namedTypedValue>
            <name>minutes</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
