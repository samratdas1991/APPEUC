    <rule>
        <name>EAP_Utils_DisplayDate</name>
        <uuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3252928</uuid>
        <description>To display date.</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
    ri!dateTime
  ),
  "",
  with(
    local!date: todate(
      ri!dateTime
    ),
    local!time: totime(
      if(
        isnull(
          ri!dateTime
        ),
        ri!dateTime,
        local(
          ri!dateTime
        )
      )
    ),
    local!result: text(local!date,"DD/MM/YYYY") &amp; " " &amp; text(local!time, "hh:mm"),
    if(
      local!result = " ",
      "n/A",
      local!result
    )
  )
)</definition>
        <namedTypedValue>
            <name>dateTime</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
