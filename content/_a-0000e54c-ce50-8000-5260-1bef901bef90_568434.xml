        </visibility>
        <definition>if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!prDynamicConfDetail),
  null,
  #"SYSTEM_SYSRULES_forEach"(
    items:ri!prDynamicConfDetail,
    expression: #"_a-0000e54c-ce50-8000-5260-1bef901bef90_568422"(
      eapPrDynamicConfDetailId:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!eapPrDynamicConfDetailId
        ),
        ri!prDynamicConfDetail[fv!index].eap_pr_dynamic_conf_detail_id,
        ri!eapPrDynamicConfDetailId
      ),
      eapPrDynamicConfId:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!eapPrDynamicConfId
        ),
        ri!prDynamicConfDetail[fv!index].eap_pr_dynamic_conf_id,
        ri!eapPrDynamicConfId
      ),
      attributeName:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!attributeName
        ),
        ri!prDynamicConfDetail[fv!index].attribute_name,
        ri!attributeName
      ),
      attributeValue:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!attributeValue
        ),
        ri!prDynamicConfDetail[fv!index].attribute_value,
        ri!attributeValue
      ),


 

    )
  )

)


</definition>
