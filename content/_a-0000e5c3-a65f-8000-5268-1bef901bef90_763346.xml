<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e5c3-a65f-8000-5268-1bef901bef90_763364</versionUuid>
    <rule>
        <name>EAP_getDocumentIDByUsingUUID</name>
        <uuid>_a-0000e5c3-a65f-8000-5268-1bef901bef90_763346</uuid>
        <description>Returns the DocumentId by UUID</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!documentUUID: tointeger(
    stripwith(
      index(
        split(
          getcontentdetailsbyuuid(
            ri!documentUUID
          ),
          ","
        ),
        2,
        {}
      ),
      "Id: "
    )
  ),
  if(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      local!documentUUID
    ),
    tointeger(
      ri!documentUUID
    ),
    local!documentUUID
  )
)</definition>
        <namedTypedValue>
            <name>documentUUID</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
