<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4cd-1af4-8000-5255-1bef901bef90_195501</versionUuid>
    <rule>
        <name>EAP_RecordFilterForMasterLookupCategory</name>
        <uuid>_a-0000e4cd-1af4-8000-5255-1bef901bef90_189988</uuid>
        <description>Record filter for master lookup category group</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!masterLookup: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_7827"(
    
    isAggregation: true(),
    isCategoryAggregation: true()
  ),
  #"SYSTEM_SYSRULES_recordFilterList"(
    name: "Category Group",
    options: #"SYSTEM_SYSRULES_forEach"(
      items: local!masterLookup,
      expression: #"SYSTEM_SYSRULES_recordFilterListOption"(
        id: fv!index,
        name: fv!item.Category,
        filter: #"SYSTEM_SYSRULES_queryFilter"(
          field: "category_group",
          operator: "=",
          value: fv!item.Category
        )
      )
    )
  )
)</definition>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
