    <versionUuid>_a-0000e521-c0a2-8000-d911-a7ef90a7ef90_2844261</versionUuid>
    <rule>
        <name>EAP_setRequestAuditModifiedBy</name>
        <uuid>_a-0000e521-c0a2-8000-d911-a7ef90a7ef90_2842889</uuid>
        <description>Set Request Audit Modified By</description>
        <parentUuid>_a-0000e505-ae05-8000-525c-1bef901bef90_421005</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  contains(
    {"ASSGN_MAKE", "SUB_MAKE", "CNCL_MAKE"},
    ri!requestCurrentStatus
  ),
  if(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!requestDetails.makerId),
    ri!createdBy,
    ri!requestDetails.makerId
  ),
  if(
    contains(
      {"ASSGN_CHCK", "REJ_CHCK", "APP_CHCK"},
      ri!requestCurrentStatus
    ),
    if(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!requestDetails.checkerId),
      ri!createdBy,
      ri!requestDetails.checkerId
    ),
    ri!createdBy
  )
)</definition>
        <namedTypedValue>
            <name>requestDetails</name>
            <type>
                <name>EAP_T_ProcessRequest</name>
                <namespace>urn:com:appian:types:EAP</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>requestCurrentStatus</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>createdBy</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
