<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_277503</versionUuid>
    <rule>
        <name>EAP_Utils_ChunkArrayHelper</name>
        <uuid>_a-0000e4cd-1af4-8000-5255-1bef901bef90_123574</uuid>
        <description>Sub rule used to create chunks of array</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  length(
    ri!list
  ) &lt; ri!chunkSize,
  ri!list,
  if(
    ri!isLast,
    ldrop(
      ri!list,
      ri!chunkSize * (
        ri!iterationIndex - 1
      )
    ),
    if(
      ri!iterationIndex = 1,
      rdrop(
        ri!list,
        length(
          ri!list
        ) - ri!chunkSize
      ),
      ldrop(
        rdrop(
          ri!list,
          length(
            ri!list
          ) - ri!chunkSize * ri!iterationIndex
        ),
        ri!chunkSize * (
          ri!iterationIndex - 1
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>list</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>chunkSize</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>iterationIndex</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>isLast</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
