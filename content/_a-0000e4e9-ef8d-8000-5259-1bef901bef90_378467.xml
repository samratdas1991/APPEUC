            label: "Cancel",
            value: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_13888"[2],
            saveInto: ri!buttonAction,
            size: "SMALL",
            style: "SECONDARY",
            submit: true(),
            validate:false()
          )
        }
      )
    },
    style: "INFO",
    showBorder: false()
  )
}</definition>
