    <rule>
        <name>EAP_DwfGetNextStepDropDownValues</name>
        <uuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_378381</uuid>
        <description>Rule for fetching the next steps drop down values in Dynamic workflow</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"_a-0000e4e9-ef8d-8000-5259-1bef901bef90_350906"(
  milestoneId: {
    index(
      #"_a-0000e4e9-ef8d-8000-5259-1bef901bef90_354337"(
        milestoneId: if(
          #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!milestoneId),
          index(
            #"_a-0000e4e9-ef8d-8000-5259-1bef901bef90_361812"(
              processControllerId: max(
                index(
                  #"_a-0000e4e9-ef8d-8000-5259-1bef901bef90_361812"(
                    processControllerMasterId: ri!manageWorkflowId
                  ),
                  "process_controller_id",
                  {}
                )
              )
            ),
            "milestone_id",
            {}
          ),
          tointeger(ri!milestoneId)
        ),
        isActive:1
      ),
      "next_milestone_id",
      {}
    )
  }
)</definition>
        <namedTypedValue>
            <name>manageWorkflowId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>milestoneId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
