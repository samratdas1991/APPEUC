<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e55f-7063-8000-d945-a7ef90a7ef90_5634215</versionUuid>
    <rule>
        <name>EAP_setRequestRtValues</name>
        <uuid>a195e62a-316f-4e0a-a0b2-dd768451c3b1</uuid>
        <description>Rule to set Request runtime value for each key in the given process</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>a!localVariables(
  local!runtimeConfigKeyData: index(
    #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_143703"(
      processId: ri!processId,
      isActive: true,
      pagingInfo: #"SYSTEM_SYSRULES_pagingInfo"(
        startIndex: 1,
        batchSize: - 1
      )
    ),
    "configRtKey",
    null
  ),
  local!runTimeFilterRequestValues: #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_143813"(
    processId: ri!processId,
    requestId: ri!requestId
  ),
  if(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      local!runtimeConfigKeyData
    ),
    {},
    if(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
        local!runTimeFilterRequestValues
      ),
      #"SYSTEM_SYSRULES_forEach"(
        items: local!runtimeConfigKeyData,
        expression: 'type!{urn:com:appian:types:EAP}EAP_T_RequestRtValue'(
          processId: ri!processId,
          requestId: ri!requestId,
          runtimeKey: fv!item,
          runtimeValue: #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_157881"(
            configRtKey: fv!item,
            requestId: ri!requestId,
            processId: ri!processId,
            makerId: ri!makerId,
            accountNumber: ri!accountNumber,
            textFilter: ri!textFilter,
            numberFilter: ri!numberFilter,
            requestMonth: ri!requestMonth,
            requestYear: ri!requestYear,
            customRunTimeKeys: ri!customRunTimeKeys
          ),
          createdBy: loggedInUser(),
          createdOn: now()
        )
      ),
      #"SYSTEM_SYSRULES_forEach"(
        items: local!runTimeFilterRequestValues,
        expression: 'type!{urn:com:appian:types:EAP}EAP_T_RequestRtValue'(
          rtValueId: fv!item.rtValueId,
          processId: ri!processId,
          requestId: ri!requestId,
          runtimeKey: fv!item.runtimeKey,
          runtimeValue: #"_a-0000e4cd-1af4-8000-5255-1bef901bef90_157881"(
            configRtKey: fv!item.runtimeKey,
            requestId: ri!requestId,
            processId: ri!processId,
            makerId: ri!makerId,
            accountNumber: ri!accountNumber,
            textFilter: ri!textFilter,
            numberFilter: ri!numberFilter,
            requestMonth: ri!requestMonth,
            requestYear: ri!requestYear,
            customRunTimeKeys: ri!customRunTimeKeys
          ),
          createdBy: loggedInUser(),
          createdOn: now()
        )
      )
    )
  )
)</definition>
        <namedTypedValue>
            <name>processId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>requestId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>makerId</name>
            <type>
                <name>string</name>
