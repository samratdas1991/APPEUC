    <rule>
        <name>EAP_Utils_GetFullNameByUserName</name>
        <uuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251844</uuid>
        <description>Returns the full name of the user by username.</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      ri!username_txt
    ),
    not(
      isusernametaken(
        ri!username_txt
      )
    )
  ),
  "",
  concat(
    user(
      ri!username_txt,
      "firstName"
    ),
    " ",
    user(
      ri!username_txt,
      "lastName"
    )
  )
)</definition>
        <namedTypedValue>
            <name>username_txt</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
