<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e56b-0831-8000-5261-1bef901bef90_703576</versionUuid>
    <rule>
        <name>EAP_RecordLink</name>
        <uuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_350812</uuid>
        <description>Contains rule to return record link</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#"SYSTEM_SYSRULES_safeLink"(
  label: ri!label,
  uri: fn!concat(
    ri!siteUrl,
    "/page/request/",
    "record/",
    #"SYSTEM_SYSRULES_recordLink"(
      recordType: ri!recordType,
      identifier: ri!identifier
    ).@attributes.@anyAttribute._recordRef,
    "/view/",
    if(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!dashboard),"summary",
    ri!dashboard)
  ),
  showWhen:ri!showWhen
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>identifier</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>siteUrl</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>dashboard</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>recordType</name>
            <type>
                <name>RecordType</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>showWhen</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
