<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e58b-f438-8000-5262-1bef901bef90_723235</versionUuid>
    <rule>
        <name>EAP_Utils_checkIsUserValidAndActive</name>
        <uuid>_a-0000e58b-f438-8000-5262-1bef901bef90_722585</uuid>
        <description>Utils rule for checking whether the input user is active and valid</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*Rule for checking whether the provided user is a valid and active in the environment*/
if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
    ri!userSOEID
  ),
  null,
  if(
    toboolean(
      isusernametaken(
        ri!userSOEID
      )
    ),
    if(
      toboolean(
        isuseractive(
          ri!userSOEID
        )
      ),
      true(),
      false()
    ),
    false()
  )
)</definition>
        <namedTypedValue>
            <name>userSOEID</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
