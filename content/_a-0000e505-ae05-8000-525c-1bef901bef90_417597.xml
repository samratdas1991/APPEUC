        <definition>if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
    ri!linkEUCTrans
  ),
  null,
  #"SYSTEM_SYSRULES_forEach"(
    items: ri!linkEUCTrans,
    expression:#"_a-0000e505-ae05-8000-525c-1bef901bef90_417523"(
      linkTransId:if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!linkTransId
        ),
        ri!linkEUCTrans[fv!index].link_trans_id,
        ri!linkTransId
      ),
      linkConfigId: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!linkConfigId
        ),
        ri!linkEUCTrans[fv!index].link_config_id,
        ri!linkConfigId
      ),    
      createdBy:if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!createdBy
        ),
        ri!linkEUCTrans[fv!index].created_by,
        ri!createdBy
      ),
      createdOn: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!createdOn
        ),
        ri!linkEUCTrans[fv!index].created_on,
        ri!createdOn
      )

    )
  )
)




</definition>
