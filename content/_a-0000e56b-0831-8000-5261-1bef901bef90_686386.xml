        </visibility>
        <definition>if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!bulkConfUploadTrans),
  null,

  #"SYSTEM_SYSRULES_forEach"(
    items: ri!bulkConfUploadTrans,
    expression: 
    'type!{urn:com:appian:types:eap}EAP_T_BulkConfUploadTrans'(
      bulk_upload_trans_id:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!bulkUploadTransId
        ),
        ri!bulkConfUploadTrans[fv!index].bulk_upload_trans_id,
        ri!bulkUploadTransId
      ), 
      bulk_upload_type: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!bulkUploadType
        ),
        ri!bulkConfUploadTrans[fv!index].bulk_upload_type,
        ri!bulkUploadType
      ), 
      tran_status:if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!tranStatus
        ),
        ri!bulkConfUploadTrans[fv!index].tran_status,
        ri!tranStatus
      ),
      uploaded_filename: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!uploadedFilename
        ),
        ri!bulkConfUploadTrans[fv!index].uploaded_filename,
        ri!uploadedFilename
      ),
      appian_doc_id: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!appianDocId
        ),
        ri!bulkConfUploadTrans[fv!index].appian_doc_id,
        ri!appianDocId
      ),
      total_record_count: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!totalRecordCount
        ),

        ri!bulkConfUploadTrans[fv!index].total_record_count,
        ri!totalRecordCount
      ),
      success_count:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!successCount
        ),

        ri!bulkConfUploadTrans[fv!index].success_count,
        ri!successCount

      ),
      failed_count: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!failedCount
        ),
        ri!bulkConfUploadTrans[fv!index].failed_count,
        ri!failedCount
      ),
      remarks: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!remarks
        ),
        ri!bulkConfUploadTrans[fv!index].remarks,
        ri!remarks
      ),
      created_by: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!createdBy
        ),

        ri!bulkConfUploadTrans[fv!index].created_by,
        ri!createdBy
      ),
      created_on:  if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!createdOn
        ),

        ri!bulkConfUploadTrans[fv!index].created_on,
        ri!createdOn

      ),
      updated_by: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!updatedBy
        ),
        ri!bulkConfUploadTrans[fv!index].updated_by,
        ri!updatedBy
      ),
      updated_on: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!updatedOn
        ),
        ri!bulkConfUploadTrans[fv!index].updated_on,
        ri!updatedOn
      )
    )
  )
)




</definition>
