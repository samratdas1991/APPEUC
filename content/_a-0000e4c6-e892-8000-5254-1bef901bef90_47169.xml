    <versionUuid>_a-0000e4cd-1af4-8000-5255-1bef901bef90_134953</versionUuid>
    <rule>
        <name>EAP_CheckForDropdownValueExistInChoiceValue</name>
        <uuid>_a-0000e4c6-e892-8000-5254-1bef901bef90_47169</uuid>
        <description>Rule to Check For Dropdown if Value Exist In ChoiceValue</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
    ri!value
  ),
  false(),
  if(
    contains(
      touniformstring(
        ri!choiceValue
      ),
      tostring(
        ri!value
      )
    ),
    true(),
    false()
  )
)</definition>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>choiceValue</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
