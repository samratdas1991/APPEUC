            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>with(
  local!dataSubset: #"SYSTEM_SYSRULES_queryEntity_v1"(
    entity: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_12843", 
    query: #"SYSTEM_SYSRULES_query"(
      selection: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!columns
        ),
        {},
        #"SYSTEM_SYSRULES_querySelection"(
          columns: #"SYSTEM_SYSRULES_forEach"(
            items: ri!columns,
            expression: #"SYSTEM_SYSRULES_queryColumn"(
              field: fv!item
            )
          )
        )
      ),
      logicalExpression: #"SYSTEM_SYSRULES_queryLogicalExpression"(
        operator: "AND",
        filters: {
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "processId",
            operator: "=",
            value: ri!processId,
            applyWhen: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_9906"(
              ri!processId
            )
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "checklistType",
            operator: "=",
            value: ri!checklistType,
            applyWhen: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_9906"(
              ri!checklistType
            )
          ),
          #"SYSTEM_SYSRULES_queryFilter"(
            field: "isActive",
            operator: "=",
            value: tointeger(
              true()
            )
          )
        }
      ),
      pagingInfo: if(
        #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!pagingInfo
        ),
        #"SYSTEM_SYSRULES_pagingInfo"(
          startIndex: 1,
          batchSize: - 1,
          sort: #"SYSTEM_SYSRULES_sortInfo"(
            field: "sortOrder",
            ascending: true()
          )
        ),
        ri!pagingInfo
      )
    ), 
    fetchTotalCount:true
  ),
  if(
    ri!returnDataSubset,
    local!dataSubset,
    cast(
      typeof(
        {
          'type!{urn:com:appian:types:EAP}EAP_T_RefChecklist'()
        }       
      ),
      local!dataSubset.data
    )
  )
)</definition>
