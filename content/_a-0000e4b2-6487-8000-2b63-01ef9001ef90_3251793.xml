    <rule>
        <name>EAP_Utils_GetValidUserOrGroupFromText</name>
        <uuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251793</uuid>
        <description>To find text is a group or user.</description>
        <parentUuid>_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3251406</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isusernametaken(
    ri!userOrGroup_txt
  ),
  touser(
    ri!userOrGroup_txt
  ),
  if(
    not(
      isusernametaken(
        runtimetypeof(
          topeople(
            ri!userOrGroup_txt
          )
        )
      )
    ),
    index(
      a!groupsByName(
        ri!userOrGroup_txt
      ),
      1,
      null
    ),
    ri!userOrGroup_txt
  )
)</definition>
        <namedTypedValue>
            <name>userOrGroup_txt</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
