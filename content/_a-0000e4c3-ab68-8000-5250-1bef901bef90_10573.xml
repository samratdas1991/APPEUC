    <interface>
        <name>EAP_GenericMilestoneField</name>
        <uuid>_a-0000e4c3-ab68-8000-5250-1bef901bef90_10573</uuid>
        <description>Holds generic milestone field.</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6580</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>{
  #"SYSTEM_SYSRULES_milestoneField"(
    label: ri!label,
    labelPosition: "ABOVE",
    steps: ri!steps,
    links: {
      #"SYSTEM_SYSRULES_forEach"(
        items: ri!steps,
        expression: if(
          or(ri!isHideNextStepsLink),
          if(
            fv!index &lt; tointeger(if(
              or(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!selectedStep),
              not(contains(ri!steps,ri!selectedStep))),
            ri!activeStep,
            wherecontains(
              tostring(ri!selectedStep),
              touniformstring(ri!steps)
            )
            )),
            if(
              ri!IsValidate=true(),
              #"SYSTEM_SYSRULES_submitLink"(
                value: #"_a-0000e4c3-ab68-8000-5250-1bef901bef90_13888"[7],
                saveInto: {
                  ri!ButtonAction,
                a!save(
                  ri!activeStep,
                  fv!index
                ),
                a!save(
                  ri!selectedStep,
                  fv!item
                ),
                ri!saveInto}
              ),
              #"SYSTEM_SYSRULES_dynamicLink"(
                saveInto: {
                  a!save(
                    ri!activeStep,
                    fv!index
                  ),
                  a!save(
                    ri!selectedStep,
                    fv!item
                  ),
                  ri!saveInto
                }
              )
            ),
           
        null),
        if(
          ri!IsValidate=true(),
          #"SYSTEM_SYSRULES_submitLink"(
            value:#"_a-0000e4c3-ab68-8000-5250-1bef901bef90_13888"[7],
            saveInto: 
            {
              ri!ButtonAction,
              a!save(
                ri!activeStep,
                fv!index
              ),
              a!save(
                ri!selectedStep,
                fv!item
              ),
              ri!saveInto
            }
          ),
          #"SYSTEM_SYSRULES_dynamicLink"(
            saveInto: {
              a!save(
                ri!activeStep,
                fv!index
              ),
              a!save(
                ri!selectedStep,
                fv!item
              ),
              ri!saveInto
            }

          )
        )
       
        )
        
      )
    },
    active: if(
      or(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!selectedStep),
      not(contains(ri!steps,ri!selectedStep))),
    ri!activeStep,
    wherecontains(
      tostring(ri!selectedStep),
      touniformstring(ri!steps)
    )
    ),
    showWhen: if(#"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(ri!steps),
    false(),
    ri!showWhen
    ),
    color: "#000080"
  )
}</definition>
