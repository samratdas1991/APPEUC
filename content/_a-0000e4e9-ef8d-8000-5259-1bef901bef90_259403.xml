    <versionUuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_261928</versionUuid>
    <rule>
        <name>EAP_GetHelpToolTip</name>
        <uuid>_a-0000e4e9-ef8d-8000-5259-1bef901bef90_259403</uuid>
        <description>rule to get HelpToolTip</description>
        <parentUuid>_a-0000e4c3-7a1b-8000-524f-1bef901bef90_6574</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  and(
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      ri!categoryGroup
    ),
    #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
      ri!uniqueKey
    )
  ),
  ri!helpToolTip,
  concat(
    "Contains values from master lookup with ",
    if(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
          ri!categoryGroup
        ),
        "",
        concat(
          "  category : ",
    ri!categoryGroup
        )
    
    ),
    if(
      #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
        ri!uniqueKey
      ),
      "",
      concat(
        if(
          #"_a-0000e4b2-6487-8000-2b63-01ef9001ef90_3233095"(
            ri!categoryGroup
          ),        
        " unique key : ",
        " and unique key : "
        ),
        ri!uniqueKey
      )

    )   
   
    )
)</definition>
        <namedTypedValue>
            <name>categoryGroup</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>uniqueKey</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>helpToolTip</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
